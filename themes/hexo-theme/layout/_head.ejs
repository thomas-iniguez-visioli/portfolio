<meta charset="utf-8">
<title><%= title(page) %></title>
<meta http-equiv="x-ua-compatible" content="ie=edge">
<script src="https://vbcq-volley.github.io/tarteaucitron/tarteaucitron.min.js"></script>
<script type="text/javascript">
    tarteaucitron.init({
    "privacyUrl": "", /* Url de la politique de confidentialité */
      "bodyPosition": "top", /* top place le bandeau de consentement au début du code html, mieux pour l'accessibilité */
  
    "hashtag": "#tarteaucitron", /* Hashtag qui permet d'ouvrir le panneau de contrôle  */
    "cookieName": "tarteaucitron", /* Nom du cookie (uniquement lettres et chiffres) */
  
    "orientation": "middle", /* Position de la bannière (top - bottom - popup - banner) */
   
      "groupServices": true, /* Grouper les services par catégorie */
      "showDetailsOnClick": true, /* Cliquer pour ouvrir la description */
      "serviceDefaultState": "wait", /* Statut par défaut (true - wait - false) */
                       
    "showAlertSmall": false, /* Afficher la petite bannière en bas à droite */
    "cookieslist": false, /* Afficher la liste des cookies */
                       
      "closePopup": true, /* Afficher un X pour fermer la bannière */
  
      "showIcon": true, /* Afficher un cookie pour ouvrir le panneau */
      //"iconSrc": "", /* Optionnel: URL ou image en base64 */
      "iconPosition": "TopRight", /* Position de l'icons: (BottomRight - BottomLeft - TopRight - TopLeft) */
  
    "adblocker": true, /* Afficher un message si un Adblocker est détecté */
                        
      "DenyAllCta" : true, /* Afficher le bouton Tout refuser */
      "AcceptAllCta" : true, /* Afficher le bouton Tout accepter */
      "highPrivacy": true, /* Attendre le consentement */
      "alwaysNeedConsent": true, /* Demander le consentement même pour les services "Privacy by design" */
  
    "handleBrowserDNTRequest": true, /* Refuser tout par défaut si Do Not Track est activé sur le navigateur */
  
    "removeCredit": true, /* Retirer le lien de crédit vers tarteaucitron.io */
    "moreInfoLink": true, /* Afficher le lien En savoir plus */
  
      "useExternalCss": false, /* Mode expert : désactiver le chargement des fichiers .css tarteaucitron */
      "useExternalJs": false, /* Mode expert : désactiver le chargement des fichiers .js tarteaucitron */
  
    //"cookieDomain": ".my-multisite-domaine.fr", /* Optionnel: domaine principal pour partager le consentement avec des sous domaines */
                      
      "readmoreLink": "", /* Changer le lien En savoir plus par défaut */
  
      "mandatory": true, /* Afficher un message pour l'utilisation de cookies obligatoires */
      "mandatoryCta": false, /* Afficher un bouton pour les cookies obligatoires (déconseillé) */
  
      //"customCloserId": "", /* Optionnel a11y: ID personnalisé pour ouvrir le panel */
      
      "googleConsentMode": true, /* Activer le Google Consent Mode v2 pour Google ads & GA4 */
      "bingConsentMode": true, /* Activer le Bing Consent Mode pour Clarity & Bing Ads */
      "softConsentMode": false, /* Soft consent mode (le consentement est requis pour charger les tags) */
  
      "dataLayer": false, /* Envoyer un événement dans dataLayer avec le statut des services */
      "serverSide": false, /* Server side seulement, les tags ne sont pas chargé côté client */
      
      "partnersList": true /* Afficher le détail du nombre de partenaires sur la bandeau */
    });
    </script>
    <script>
      tarteaucitron.user.tawktoId = '6852762ef868b7190e3fca09';
      (tarteaucitron.job = tarteaucitron.job || []).push('tawkto');
      tarteaucitron.user.umamiId = '41565600-ce48-4a41-9241-dd1e7a373da6';
      (tarteaucitron.job = tarteaucitron.job || []).push('umami');
      </script>
     <script>

    
        tarteaucitron.services.umami = {
          "key": "umami",
          "type": "analytic",
          "name": "Umami",
          "needConsent": true,
          "cookies": ['umami'],
          "readmoreLink": "https://umami.is/",
          "js": function () {
            "use strict";
            const u=document.createElement('script');
            u.defer=true;
            u.src='https://cloud.umami.is/script.js';
            u.dataset.websiteId='3114a86e-6e66-4953-b19f-40491a63d444';
            document.head.appendChild(u);
          },
          "fallback": function () {
            "use strict";
            console.log('umami');
            // do nothing
          },
        };
        </script>
<!-- Performance optimized font loading system with preload and font-display: swap -->
<%- optimizedFonts() %>

<!-- Critical resource preloading -->
<link rel="preload" href="/assets/icons.svg" as="image">
<link rel="preload" href="/assets/avatar.avif" as="image">
<link rel="preload" href="/js/performance.js" as="script">

<!-- Critical CSS inlined for immediate rendering -->
<%- partial('_critical-css') %>

<!-- Optimized CSS loading: critical inline, non-critical lazy loaded -->
<%- optimizedCSS() %>

<!-- Developer theme specific CSS -->
<link rel="preload" href="<%= url_for('/css/dev-theme.css') %>" as="style" onload="this.onload=null;this.rel='stylesheet'">
<link rel="preload" href="<%= url_for('/css/dev-components.css') %>" as="style" onload="this.onload=null;this.rel='stylesheet'">
<link rel="preload" href="<%= url_for('/css/dev-theme-fixes.css') %>" as="style" onload="this.onload=null;this.rel='stylesheet'">

<!-- Fallback for legacy CSS loading -->
<noscript>
  <%- cssCache(page_css(page)) %>
  <link rel="stylesheet" href="<%= url_for('/css/dev-theme.css') %>">
  <link rel="stylesheet" href="<%= url_for('/css/dev-components.css') %>">
  <link rel="stylesheet" href="<%= url_for('/css/dev-theme-fixes.css') %>">
</noscript>
<meta name="viewport" content="initial-scale=1.0">
<!-- generated via https://realfavicongenerator.net -->
<link rel="icon" type="image/png" href="/assets/favicon-48x48.png" sizes="48x48" />
<link rel="icon" type="image/svg+xml" href="/assets/favicon.svg" />
<link rel="shortcut icon" href="/assets/favicon.ico" />
<link rel="apple-touch-icon" sizes="180x180" href="/assets/apple-touch-icon.png" />
<meta name="apple-mobile-web-app-title" content="thom4.net" />
<link rel="manifest" href="/assets/site.webmanifest" />
<meta name="theme-color" content="#FFF9F4">
<meta name="description" content="<%= description(page) %>">
<meta name="keywords" content="<%= (page.tags && page.tags.toArray().map(tag => tag.name) || []).concat(config.keywords).join(',') %>">
<link rel="alternate" type="application/rss+xml" title="<%= [config.title].join(' • ') %>" href="https://thom4.net/feed/">
<link rel="alternate" type="application/rss+xml" title="<%= [config.title, 'Photography'].join(' • ') %>" href="https://thom4.net/photography/feed/">
<link rel="alternate" type="application/rss+xml" title="<%= [config.title, 'Talks'].join(' • ') %>" href="https://thom4.net/talks/feed/">
<link rel="author" href="<%= theme.author %>">
<% if (is_home() || is_archive()){ -%><link rel="prefetch" href="<%= page.posts.first().permalink %>"><% } %>
<% if (page.next){ -%><link rel="prefetch" href="<%= page.next.permalink %>"><% } %>
<% if (page.prev){ -%><link rel="prefetch" href="<%= page.prev.permalink %>"><% } %>
<meta name="fediverse:creator" content="<%= theme.graph.fediverse_creator %>">
<meta name="twitter:card" content="summary">
<meta name="twitter:site" content="@<%= theme.graph.twitter %>">
<meta name="twitter:creator" content="@<%= theme.graph.twitter %>">
<meta name="twitter:image" content="<%= page.image || config.default_cover %>">
<meta name="twitter:description" content="<%= description(page) %>">
<meta property="og:title" content="<%= page.title || [config.title, config.subtitle].join(' &mdash; ') %>">
<meta property="og:type" content="article">
<meta property="og:url" content="<%= url_for(page.permalink) %>">
<meta property="og:image" content="<%= page.image || config.default_cover %>">
<meta property="og:description" content="<%= description(page) %>">
<meta property="og:locale" content="fr_FR">
<meta property="og:locale:alternate" content="en_GB">
<meta property="og:site_name" content="<%= config.title %>">
<link rel="me" href="https://twitter.com/<%= theme.graph.twitter %>">
<link rel="me authn" href="<%= theme.graph.mastodon %>">
<link rel="me authn" href="<%= theme.graph.github %>">
<link rel="me authn" href="<%= theme.graph.gitlab %>">
<link rel="authorization_endpoint" href="https://indieauth.com/auth">
<link rel="me" href="<%= theme.graph.framagit %>">
<link rel="me" href="<%= theme.graph.pro %>">
<link rel="me" href="<%= theme.graph.old %>">
<% if (theme.webmentions) { -%>
<% Object.entries(theme.webmentions.features).forEach(f => { -%>
<link rel="<%= f[0] %>" href="<%= theme.webmentions.url %>/<%= f[1] %>">
<% }) -%>
<% } -%>

<%- (page.scripts || []).map(script => `<script type="module" src="/assets/js/${script}"></script>`).join('\n') %>

<!-- Modern navigation script -->
<script type="module" src="<%= url_for('/js/navigation.js') %>"></script>

<!-- Modern cards script -->
<script type="module" src="<%= url_for('/js/cards.js') %>"></script>

<!-- Modern animations script -->
<script type="module" src="<%= url_for('/js/animations.js') %>"></script>

<!-- Performance optimizations script -->
<script type="module" src="<%= url_for('/js/performance.js') %>"></script>

<!-- Accessibility enhancements script -->
<script type="module" src="<%= url_for('/js/accessibility.js') %>"></script>

<!-- Accessibility testing script (development only) -->
<% if (process.env.NODE_ENV !== 'production') { %>
<script type="module" src="<%= url_for('/js/accessibility-tests.js') %>"></script>
<% } %></script>

<!-- Sidebar and simplified footer styles -->
<link rel="stylesheet" href="<%= url_for('/css/sidebar.css') %>">
<link rel="stylesheet" href="<%= url_for('/css/footer-simple.css') %>">

<!-- Newsletter form styles -->
<link rel="stylesheet" href="<%= url_for('/css/newsletter.css') %>">

<!-- Content component styles -->
<link rel="stylesheet" href="<%= url_for('/css/cards.css') %>">
<link rel="stylesheet" href="<%= url_for('/css/buttons.css') %>">
<link rel="stylesheet" href="<%= url_for('/css/navigation.css') %>">
<link rel="stylesheet" href="<%= url_for('/css/forms.css') %>">

<!-- Animation and transition styles -->
<link rel="stylesheet" href="<%= url_for('/css/animations.css') %>">

<!-- Content components JavaScript -->
<script type="module" src="<%= url_for('/js/content-components.js') %>"></script>
