<!DOCTYPE html>
<html lang="<%- config.language %>" data-fr-scheme="system">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="format-detection" content="telephone=no,date=no,address=no,email=no,url=no">
    <link rel="stylesheet" href="<%- url_for('/css/tac.css') %>">
  <script src="https://vbcq-volley.github.io/tarteaucitron/tarteaucitron.min.js"></script>

  <script type="text/javascript">
  tarteaucitron.init({
  "privacyUrl": "",
    "bodyPosition": "top",
  "hashtag": "#tarteaucitron",
  "cookieName": "tarteaucitron",
  "orientation": "middle",
    "groupServices": true,
    "showDetailsOnClick": true,
    "serviceDefaultState": "wait",
  "showAlertSmall": false,
  "cookieslist": false,
    "closePopup": true,
    "showIcon": true,
    "iconPosition": "TopRight",
  "adblocker": true,
    "DenyAllCta" : true,
    "AcceptAllCta" : true,
    "highPrivacy": true,
    "alwaysNeedConsent": true,
  "handleBrowserDNTRequest": true,
  "removeCredit": true,
  "moreInfoLink": true,
    "useExternalCss": true,
    "useExternalJs": false,
    "readmoreLink": "",
    "mandatory": true,
    "mandatoryCta": false,
    "googleConsentMode": true,
    "bingConsentMode": true,
    "softConsentMode": false,
    "dataLayer": false,
    "serverSide": false,
    "partnersList": true
  });
  </script>
  <script>
    tarteaucitron.user.tawktoId = '6852762ef868b7190e3fca09';
    (tarteaucitron.job = tarteaucitron.job || []).push('tawkto');
  
    </script>
  
    <meta name="theme-color" content="#0d1117">
    <link rel="apple-touch-icon" href="<%- url_for('/favicon/apple-touch-icon.png') %>">
    <link rel="icon" href="<%- url_for('/favicon/favicon.svg') %>" type="image/svg+xml">
    <link rel="shortcut icon" href="<%- url_for('/favicon/favicon.ico') %>" type="image/x-icon">
    <link rel="manifest" href="<%- url_for('/favicon/manifest.webmanifest') %>" crossorigin="use-credentials">

    <link rel="stylesheet" href="<%- url_for('/css/dsfr.min.css') %>">
    <link rel="stylesheet" href="<%- url_for('/css/utility.min.css') %>">
    <link rel="stylesheet" href="<%- url_for('/css/style.css') %>">

    <script src="<%- url_for('/js/module-loader.js') %>" defer></script>

    <!-- Anti-flash dark mode -->
    <script type="module">
      const e="system",t="dark",c="dark",o="data-fr-theme",a="data-fr-scheme",r=`:root[${o}], :root[${a}]`,m=()=>{document.documentElement.setAttribute(o,c),document.documentElement.style.colorScheme="dark"},n=()=>{window.matchMedia("(prefers-color-scheme: dark)").matches&&m()};(()=>{if(document.documentElement.matches(r)){const c=(()=>{try{return"localStorage"in window&&null!==window.localStorage}catch(e){return!1}})()?localStorage.getItem("scheme"):"",o=document.documentElement.getAttribute(a);switch(!0){case c===t:m();break;case c===e:n();break;case o===t:m();break;case o===e:n()}}})();
    </script>

  <title><%- page.title ? page.title + ' | ' : '' %><%- config.title %></title>

  <!-- Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600&family=VT323&display=swap" rel="stylesheet">

</head>

<body>

<header role="banner" class="fr-header">
  <div class="fr-header__body">
    <div class="fr-container">
      <div class="fr-header__body-row">
        <div class="fr-header__brand fr-enlarge-link">
          <img src="<%- url_for('/photo.jpg') %>" alt="Photo de <%- config.author %>" width="56" height="56" style="border-radius:50%;object-fit:cover;margin-right:1rem;vertical-align:middle;"/>
          <div class="fr-header__brand-top">
            <div class="fr-header__logo">
              <a href="/" title="Retour à l'accueil">
                <p class="fr-logo">
                  <%- theme.glitch_text || config.author %>
                </p>
              </a>
            </div>
            <div class="fr-header__navbar">
              <button data-fr-opened="false" aria-controls="modal-header" title="Menu" type="button" id="button-header" class="fr-btn--menu fr-btn">Menu</button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="fr-header__menu fr-modal" id="modal-header" aria-labelledby="button-header">
    <div class="fr-container">
      <button aria-controls="modal-header" title="Fermer" type="button" id="button-2168" class="fr-btn--close fr-btn">Fermer</button>
      <div class="fr-header__menu-links"></div>
      <nav class="fr-nav" role="navigation" aria-label="Menu principal">
        <ul class="fr-nav__list">
          <% for (var i in theme.menu) { %>
            <li class="fr-nav__item">
              <a type="link" href="<%- url_for(theme.menu[i]) %>" class="fr-nav__link">[<%- i %>]</a>
            </li>
          <% } %>
        </ul>
      </nav>
    </div>
  </div>
</header>

<div class="fr-container--fluid">
  <div class="fr-grid-row">
    <div class="fr-col-1"></div>
    <div class="fr-col-7">
      <main class="site-main" id="main-content">
        <!-- Barre de statut terminale -->
        <div class="status-bar" id="status-bar">
          SYSTÈME: portfolio.v3 | thomas-iniguez-visioli | FR
        </div>
        <div class="main-content">
          <%- body %>
        </div>
      </main>
    </div>
    <div class="fr-col-3">
      <aside>
        <%- partial('_partial/sidebar') %>
      </aside>
    </div>
    <div class="fr-col-1"></div>
  </div>
</div>

<footer class="fr-footer" role="contentinfo">
  <div class="fr-container">
    <div class="fr-footer__body">
      <div class="fr-footer__brand fr-enlarge-link">
        <a id="footer-brand-link" title="Retour à l'accueil" href="/">
          <p class="fr-logo">thomas iniguez visioli</p>
        </a>
      </div>
      <div class="fr-footer__content">
        <p class="fr-footer__content-desc">
          Développeur passionné · portfolio généré avec Hexo · thème matrix-flow
        </p>
        <ul class="fr-footer__content-list">
          <% if (theme.social && theme.social.github) { %>
          <li class="fr-footer__content-item">
            <a class="fr-footer__content-link" target="_blank" rel="noopener" href="https://github.com/<%- theme.social.github %>">GitHub</a>
          </li>
          <% } %>
          <% if (theme.social && theme.social.linkedin) { %>
          <li class="fr-footer__content-item">
            <a class="fr-footer__content-link" target="_blank" rel="noopener" href="https://linkedin.com/in/<%- theme.social.linkedin %>">LinkedIn</a>
          </li>
          <% } %>
          <% if (theme.social && theme.social.email) { %>
          <li class="fr-footer__content-item">
            <a class="fr-footer__content-link" href="mailto:<%- theme.social.email %>">Email</a>
          </li>
          <% } %>
        </ul>
      </div>
    </div>
    <div class="fr-footer__bottom">
      <ul class="fr-footer__bottom-list">
        <li class="fr-footer__bottom-item">
          <a href="#" class="fr-footer__bottom-link">Plan du site</a>
        </li>
        <li class="fr-footer__bottom-item">
          <a href="#" class="fr-footer__bottom-link">Mentions légales</a>
        </li>
        <li class="fr-footer__bottom-item">
          <a href="#tarteaucitron" class="fr-footer__bottom-link">Gestion des cookies</a>
        </li>
      </ul>
      <div class="fr-footer__bottom-copy">
        <p>&copy; <%= new Date().getFullYear() %> <%- config.author %></p>
      </div>
    </div>
  </div>
</footer>

<script>tarteaucitron.user.tawktoWidgetId = '1iu11r9a9';</script>
<script type="module" src="<%- url_for('/js/dsfr.module.min.js') %>"></script>
<script type="text/javascript" nomodule src="<%- url_for('/js/dsfr.nomodule.min.js') %>"></script>

</body>
</html>