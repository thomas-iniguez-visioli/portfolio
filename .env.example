# Discord Webhook Newsletter Configuration
# Copy this file to .env and fill in your actual webhook URL

# Discord webhook URL for newsletter subscriptions
# Get this from your Discord server settings > Integrations > Webhooks
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Environment configuration
NODE_ENV=development

# Build optimization settings (optional overrides)
BUILD_TIMESTAMP_THRESHOLD=24

# Newsletter settings (optional overrides)
NEWSLETTER_RETRY_ATTEMPTS=3
NEWSLETTER_TIMEOUT_MS=5000
NEWSLETTER_RATE_LIMIT_MS=1000